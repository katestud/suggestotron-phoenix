<h2><%= @venue.name %></h2>

<ul>

  <li>
    <strong>Location:</strong>
    <%= @venue.location %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @venue.description %>
  </li>

  <li>
    <strong>Categories:</strong>
    <%= category_list(@venue) %>
  </li>

</ul>

<%= link "Edit", to: venue_path(@conn, :edit, @venue) %> |
<%= link "Back", to: venue_path(@conn, :index) %>
